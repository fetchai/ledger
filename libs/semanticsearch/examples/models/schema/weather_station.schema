model Position {
  latitude: BoundedInteger(-90, 90),
  longitude: BoundedInteger(0, 180)  
};

model WeatherStation
{
  position: Position,
  temperature: BoundedInteger(-180, 300)
};

let x : WeatherStation = {
  position: {
    latitude: 0,
    longitude: 1
  },
  temperature: 9
};

advertise x until 200132;

let y :  Position = {
    latitude: 9,
    longitude: 20
};

find y direction x granularity 7;